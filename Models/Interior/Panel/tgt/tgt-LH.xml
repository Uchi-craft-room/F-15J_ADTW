<?xml version="1.0"?>

<PropertyList>

    <path>tgt.ac</path>

    <animation>
        <type>material</type>
        <object-name>Prpm.1</object-name>
        <object-name>Prop.ind</object-name>
     <emission>
      <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
      <red>   0.5 </red>
      <green> 0.2 </green>
      <blue>  0.2 </blue>
    </emission>
  </animation>

    <animation>
        <type>material</type>
        <object-name>PropRPM.face</object-name>
        <object-name>PropRPMneedle</object-name>
        <object-name>drum-4</object-name>
        <object-name>drum-3</object-name>
        <object-name>drum-2</object-name>
        <object-name>drum-1</object-name>    
     <emission>
      <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
      <red>   0.5 </red>
      <green> 0.5 </green>
      <blue>  0.0 </blue>
    </emission>
  </animation>


    <animation>
        <type>rotate</type>
        <object-name>PropRPMneedle</object-name>
        <property>engines/engine[1]/itt-norm</property>
        <interpolation>
            <entry><ind>0</ind><dep>0</dep></entry>>
            <entry><ind>0.200</ind><dep>0</dep></entry>
            <entry><ind>0.600</ind><dep>90</dep></entry>
            <entry><ind>1.000</ind><dep>180</dep></entry>
            <entry><ind>1.400</ind><dep>270</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>


<!--  drum counter -->


 <animation>
  <type>textranslate</type>
  <object-name>drum-3</object-name>
  <property>engines/engine[1]/itt-norm</property>
  <factor>10</factor>
  <offset>-0.01</offset>
  <step>0.01</step>
  <scroll>0.01</scroll>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>



 <animation>
  <type>textranslate</type>
  <object-name>drum-2</object-name>
  <property>engines/engine[1]/itt-norm</property>
  <factor>1</factor>
  <offset>-0.1</offset>
  <step>0.1</step>
  <scroll>0.01</scroll>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>



 <animation>
  <type>textranslate</type>
  <object-name>drum-1</object-name>
  <property>engines/engine[1]/itt-norm</property>
  <factor>0.1</factor>
  <offset>-1</offset>
  <step>1</step>
  <scroll>0.01</scroll>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>



</PropertyList>

