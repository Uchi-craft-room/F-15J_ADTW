<!-- Pushback -->
<model>
 <name>Pushback</name>
 <path>Aircraft/F-15J_ADTW/Models/towing_car/towing_car.xml</path>
  <condition>
 <less-than-equals>
     <property>velocities/airspeed-kt</property>
     <value>80</value>
 </less-than-equals>
  </condition>
   <offsets>
            <x-m>-4.50</x-m>
            <y-m>-0.00</y-m>
            <z-m>-2.2</z-m>
 </offsets>
</model> 

  <animation>
    <object-name>Pushback</object-name>
    <nopreview/>
  </animation>

