 <!-- intake -->
  <animation>
    <type>rotate</type>
    <object-name>intake-RH</object-name>
    <property>controls/engines/engine/starter-position-norm</property>
    <interpolation>
      <entry><ind>  0.0 </ind><dep>  0.0 </dep></entry>
      <entry><ind>  0.98 </ind><dep> 0.0 </dep></entry>
      <entry><ind>  1.0 </ind><dep> 11.0 </dep></entry>
    </interpolation>
    <center>
      <x-m>-3.293 </x-m>
      <y-m> 0.000 </y-m>
      <z-m>-0.294 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>


  <animation>
    <type>rotate</type>
    <object-name>intake-LH</object-name>
    <property>controls/engines/engine[1]/starter-position-norm</property>
    <interpolation>
      <entry><ind>  0.0 </ind><dep>  0.0 </dep></entry>
      <entry><ind>  0.98 </ind><dep> 0.0 </dep></entry>
      <entry><ind>  1.0 </ind><dep> 11.0 </dep></entry>
    </interpolation>
    <center>
      <x-m>-3.293 </x-m>
      <y-m> 0.000 </y-m>
      <z-m>-0.294 </z-m>
    </center>
    <axis>
